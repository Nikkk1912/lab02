#include <iostream>

using namespace std;

int square(int a)
{
    int b, c;
    b = a * a;
    c = a * a * a;
    cout << a << "||" << b << "||" << c << endl;
    return 0;
}


int main()
{
    int a, b;

    cout << "Enter lower limit: ";
    cin >> b;
    cout << "Enter upper limit: ";
    cin >> a;
    


        for (b; b <= a; b++) {
            square(b);

        }
    
}
